{
  "name": "react-redux-express-template",
  "version": "0.0.0",
  "description": "Simple template for serving a react-redux app via express",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "devstart": "nodemon",
    "debug": "nodemon --exec node --inspect=3001 --trace-warnings ./bin/www",
    "lint": "./node_modules/.bin/eslint .",
    "test": "jest",
    "build:win": "./node_modules/.bin/webpack --config webpack.config.prod.js --bail",
    "build": "npm run clean && NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.js --bail",
    "clean": "./node_modules/.bin/rimraf public/js"
  },
  "jest": {
    "testRegex": "/test/(?!.eslintrc.js$)(?!testutils.jsx$).*.jsx?$",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/public/**",
      "!**/coverage/**"
    ],
    "globals": {
      "window": {}
    },
    "coverageReporters": [
      "text-summary",
      "html",
      "text"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "express": "~4.17.3",
    "express-react-views": "^0.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.6.5",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "superagent": "^5.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "jest": "^24.8.0",
    "node-sass": "^4.11.0",
    "react-test-renderer": "^16.8.6",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-hot-middleware": "^2.24.3"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  }
}
